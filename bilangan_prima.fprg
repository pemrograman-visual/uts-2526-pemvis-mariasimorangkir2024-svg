    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="n, return, i" type="Integer" array="False" size=""/>
            <input variable="n"/>
            <assign variable="x" expression="2"/>
            <for variable="i" start="2" end="n" direction="inc" step="1">
                <if expression="isPrima(i)">
