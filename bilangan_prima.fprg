    <function name="isPrima" type="Boolean" variable="return">
        <parameters>
            <parameter name="n" type="Integer" array="False"/>
        </parameters>
        <body>
            <declare name="return" type="Boolean" array="False" size=""/>
            <declare name="j" type="Integer" array="False" size=""/>
            <if expression="n&lt;=1">
                <then>
                    <assign variable="return" expression="False"/>
                </then>
                <else>
                    <assign variable="j" expression="2"/>
                    <for variable="i" start="2" end="" direction="inc" step="1"/>
                </else>
            </if>
        </body>
    </function>
